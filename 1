#include <iostream>
#include <cmath>
#include <random>
#include <vector>

 void print ( const std :: vector < std :: vector <int > > & matrix )

 {
     for(int i = 0; i < matrix.size(); i++)
     {
         for(int j = 0; j < matrix.size(); j++)
            std::cout << matrix[i][j] << ' ';
         std::cout << std::endl;

     }
 }

 int main()
 {
     size_t n = 5;
     std::vector<std::vector<int >>matrix(n,std::vector<int >(n));
     print(matrix);

     std :: mt19937_64 gen ;
     gen . seed (1) ;
     std :: uniform_int_distribution <int > dist (0 , 9) ;

     for(int i = 0; i < matrix.size(); i++)
     {
         for(int j = 0; j < matrix.size(); j++)
         {
            matrix[i][j] = dist(gen);
            std::cout << matrix[i][j]<< ' ';
         }
         std::cout << std::endl;

     }
